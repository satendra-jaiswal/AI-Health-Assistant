{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/(.*\\.(css|js|jpg|png|gif|svg|webp|html))",
      "dest": "public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ]
}